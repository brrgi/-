import sys
input=sys.stdin.readline
n,m=map(int,input().split())
c=list(map(int,input().split()))
end=max(c)
start=0

while start<=end:
    mid=(start+end)//2

    sum=0
    for i in c:
        if i>=mid:
            sum+=i-mid

    if sum>=m:
        start=mid+1
    else:
        end=mid-1
print(end)
